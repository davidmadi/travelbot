{
  "name": "@ibm-watson/assistant-simple",
  "description": "A simple Node.js based web app which shows how to use the Watson Assistant API to recognize user intents.",
  "version": "0.1.1",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test-integration": "casperjs test ./test/integration/test.*.js",
    "test-integration-runner": "NODE_ENV=test node casper-runner.js",
    "test": "mocha --compilers js:babel-core/register",
    "test-unit": "jest test/unit --coverage",
    "lint": "eslint .",
    "autofix": "eslint --fix .",
    "codecov": "npm run test && (codecov || true)"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/watson-developer-cloud/assistant-simple.git"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "htmlparser2": "^3.9.2",
    "node-fetch": "^2.2.0",
    "string-similarity": "^1.2.2",
    "watson-developer-cloud": "^3.7.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-expo": "^4.0.0",
    "babel-register": "^6.26.0",
    "casperjs": "^1.1.4",
    "chai": "^4.1.2",
    "codecov": "^3.0.4",
    "eslint": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-mocha": "^6.0.0",
    "jest": "^23.4.1",
    "phantomjs-prebuilt": "^2.1.16",
    "supertest": "^3.1.0"
  }
}
